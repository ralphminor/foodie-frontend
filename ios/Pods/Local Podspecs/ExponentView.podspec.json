    ],
    "CocoaLumberjack": [
      "~> 2.3"
    ],
    "Crashlytics": [
      "~> 3.8"
    ],
    "FBSDKCoreKit": [
      "~> 4.15"
    ],
    "FBSDKLoginKit": [
      "~> 4.15"
    ],
    "FBSDKShareKit": [
      "~> 4.15"
    ],
    "Fabric": [
      "~> 1.6"
    ],
    "Google/SignIn": [
      "~> 3.0"
    ],
    "GoogleMaps": [
      "~> 2.2.0"
    ],
    "lottie-ios": [
      "~> 1.0"
    ]
