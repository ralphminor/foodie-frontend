{
  "name": "ExponentView",
  "version": "1.13.1",
  "summary": "Exponent",
  "description": "Exponent lets web developers build truly native apps that work across both iOS and Android by writing them once in just JavaScript.",
  "homepage": "http://docs.getexponent.com",
  "license": "BSD",
  "authors": "650 Industries, Inc.",
  "requires_arc": true,
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "http://github.com/exponent/exponent.git"
  },
  "source_files": "ios/Exponent/**/*.{h,m}",
  "preserve_paths": "ios/Exponent/**/*.{h,m}",
